@model _20170328prework.ViewModels.TestActionViewModel

<h2>Test Search View</h2>

<form class="form has-feedback" id="testform" role="form">
    <div class="form-group">
        <label class="control-label">測試</label>
        <input type="text" class="form-control" placeholder="請輸入測試" id="Test" value="@Model.Test" required data-required-error="測試也是要填啊!" data-equals data-cusmin="5" data-cusmax="20" />
        <div class="help-block with-errors"></div>
    </div>
    <div class="form-group">
        <label class="control-label">電話</label>
        <input type="text" class="form-control" placeholder="請輸入電話" id="PhoneNumber" value="@Model.PhoneNumber" required />
    </div>
    <div class="form-group">
        <button onclick="GoSearch()" class="btn btn-primary">查詢</button>
    </div>
</form>
