@model _20170328prework.ViewModels.ToolBarViewModel

<div class="col-md-4 col-md-offset-8" id="toolbar">
    <nav>
        <ul class="list-inline">
            <li style="padding:0px">
                <button class="btn btn-warning" onclick="ToolBtnClick('Search')" id="Search" disabled="@(Model.DisSearch ?? false)" data-usercontrol="@(Model.DisSearch ?? false)" data-url="@Url.Action("Search",Model.ControllerName)" data-display="searchpanel">查詢</button>
            </li><!--
            --><li style="padding:0px">
                   <button class="btn btn-danger" onclick="ToolBtnClick('Add')" id="Add" disabled="@(Model.DisNew ?? false)" data-usercontrol="@(Model.DisNew ?? false)" data-url="@Url.Action("Add",Model.ControllerName)" data-display="display">新增</button>
            </li><!--
            --><li style="padding:0px">
                   <a class="btn btn-default" id="Check" disabled="@(Model.DisCheck ?? false)" data-usercontrol="@(Model.DisCheck ?? false)" href="@Url.Action("Check",Model.ControllerName)">確認</a>
            </li><!--
            --><li style="padding:0px">
                   <a class="btn btn-info" id="Delete" disabled="@(Model.DisDelete ?? false)" data-usercontrol="@(Model.DisDelete ?? false)" href="@Url.Action("Delete",Model.ControllerName)">刪除</a>
            </li><!-- 
            --><li style="padding:0px">
                   <a class="btn btn-success" id="Failed" disabled="@(Model.DisFailed ?? false)" data-usercontrol="@(Model.DisFailed ?? false)" href="@Url.Action("Failed",Model.ControllerName)">作廢</a>
            </li><!--            
            --><li style="padding:0px">
                   <a class="btn btn-primary" id="Attachment" disabled="@(Model.DisAttach ?? false)" data-usercontrol="@(Model.DisAttach ?? false)" href="@Url.Action("Attachment",Model.ControllerName)">附件</a>
            </li>
        </ul>
    </nav>
</div>
<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4><strong>查詢</strong></h4>
            </div>
            <div class="modal-body" id="searchpanel">

            </div>
        </div>
    </div>
</div>
